{
    "functions": {
        "api/app_php.php": {
          "runtime": "vercel-php@0.1.0"
        }
      },
    "builds": [
        {
            "src": "**/*.html",
            "use": "@now/static"
        },
        {
            "src": "**/*.py",
            "use": "@now/python"
        },
        {
            "src": "**/*.js",
            "use": "@now/node"
        },
        {
            "src": "**/*.php",
            "use": "@now/php"
        },
        {
            "src": "**/*.go",
            "use": "@now/go"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/python",
            "dest": "api/app_python.py"
        },
        {
            "src": "/js",
            "dest": "api/app_js.js"
        },
        {
            "src": "/php",
            "dest": "api/app_php.php"
        },
        {
            "src": "/go",
            "dest": "api/app_go.go"
        }
    ]
}

