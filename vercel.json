{
    "functions": {
        "api/*.php": { "runtime": "vercel-php@0.3.1" },
        "api/*.sh": { "runtime": "vercel-bash" }
    },
    "routes": [
        {
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/python",
            "dest": "api/app_python.py"
        },
        {
            "src": "/js",
            "dest": "api/app_js.js"
        },
        {
            "src": "/go",
            "dest": "api/app_go.go"
        },
        {
            "src": "/rb",
            "dest": "api/app_rb.rb"
        },
        {
            "src": "/php",
            "dest": "api/app_php.php"
        },
        {
            "src": "/bash",
            "dest": "api/bash/build.sh"
        },
        {
            "src": "/favicon.ico",
            "dest": "https://juicy.vercel.app/favicon.ico"
        }
    ]
}

